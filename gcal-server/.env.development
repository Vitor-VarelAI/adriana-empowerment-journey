PORT=3001
NODE_ENV=development
CLIENT_ORIGIN=http://localhost:8081

# Google OAuth2 (Development)
# Use your DEV OAuth client from Google Cloud Console
GOOGLE_CLIENT_ID=your-dev-google-client-id.apps.googleusercontent.com

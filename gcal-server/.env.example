# Google OAuth2
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
# Must match what is configured in Google Cloud Console OAuth credentials
# Example local redirect: http://localhost:3000/auth/callback
# Production redirect: https://espacoadriana.pt/auth/callback
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:3000/auth/callback

# The email/account that will be used (Adriana)
ADMIN_EMAIL=adriana@example.com

# Server settings
PORT=3000
NODE_ENV=development
CLIENT_ORIGIN=http://localhost:8080
